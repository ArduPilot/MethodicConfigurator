{
  "extends": ["config:recommended"],
  "enabledManagers": ["pre-commit"],
  "repositories": ["ArduPilot/MethodicConfigurator"],
  "packageRules": [
    {
      "matchManagers": ["pre-commit"],
      "automerge": true,
      "pinDigests": true,
      "rangeStrategy": "pin"
    }
  ],
  "pin": {
    "enabled": true
  }
}
